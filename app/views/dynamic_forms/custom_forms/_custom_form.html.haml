%tr
  %td
    = custom_form.name
  %td
    - if custom_form.submissions.count > 0
      = link_to custom_form.submissions.count, custom_form_submissions_path(custom_form), class: 'btn btn-link btn-sm text-primary'
    - else
      0
  %td
    = is_active? custom_form.enabled
  %td
    = link_to 'Edit', edit_custom_form_path(custom_form), class: 'btn btn-link btn-sm text-success'
